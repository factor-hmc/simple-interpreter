view : Float -> Svg msg
view t =
    svg
        [ viewBox "0 0 115.02 115.02", css Styles.logo ]
        [ defs
            []
            [ radialGradient
                [ id "gradient" ]
                [ stop [ stopColor "#ffee00", offset "0" ] []
                , stop [ stopColor "#ebc72c", offset "1" ] []
                ]
            ]
        , path
            [ d <| showMoves (raptor t)
            , fill "url(#gradient)"
